{
    "name": "webasics/framework",
    "description": "Next gen framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Flo Knapp",
            "email": "office@florianknapp.de"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "Webasics\\Framework\\": "src/",
            "Webasics\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "tflori/orm": "1.8.*",
        "psr/http-message": "1.0.*",
        "psr/http-factory": "1.0.*",
        "psr/http-server-handler": "1.0.*",
        "psr/http-server-middleware": "1.0.*",
        "psr/http-client": "1.0.*",
        "psr/event-dispatcher": "1.0.*",
        "psr/container": "1.0.*",
        "nyholm/psr7": "1.2.*",
        "nyholm/psr7-server": "0.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.0.*"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit -c phpunit.xml "
    },
    "minimum-stability": "stable"
}
